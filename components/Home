import Script from 'next/script'

function Home() {
  return (
    <div class="container">
     <script src="https://unpkg.com/blip-chat-widget@1.6.*" type="text/javascript"></script>
<script>
    (function () {
        window.onload = function () {
            new BlipChat()
            .withAppKey('YOUR-APP-KEY')
            .withButton({"color":"#2CC3D6"})
            .build();
        }
    })();
</script>
      <div>Home Page</div>
    </div>
  )
}

